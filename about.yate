PAGENAME = "about"
include "common.yate"

match .* content {
    apply .index page-index
}

match .index page-index {
    <div class="page-index">
        <img class="page-index__photo" src="/img/cuker2.jpg" alt=""/>
        <div class="page-index__content">
            html(.content)
        </div>
    </div>
}
