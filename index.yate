PAGENAME = "index"
include "common.yate"

match .* content {
    apply .index page-index
}

match .index page-index {
    <div class="page-index">
        if .photo {
            <img class="page-index__photo" src="/img/{ .photo }" alt=""/>
        }
        <div class="page-index__content text">
            html(.content)
        </div>
    </div>
}
